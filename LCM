include<stdio.h>
#include<conio.h>
void main()
{
    int a,b;
    printf("enter the number");
    scanf("%d%d",&a,&b);
    int big=a>b?a:b;
    int step=big;
    while(1)
    {
        if(big%a==0 && big%b==0)
            break;
        else
            big=big+step;
    }
    printf("%d LCM",big);
}
